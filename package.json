{
  "name": "unplugin-auto-import-api",
  "version": "0.0.0",
  "type": "module",
  "description": "Auto import APIs on-demand for Vite, Webpack and Rollup",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "keywords": [
    "unplugin",
    "vite-plugin",
    "webpack-plugin",
    "esbuild-plugin",
    "rollup-plugin",
    "astro-plugin"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch src",
    "build:fix": "esno scripts/postbuild.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.2",
    "local-pkg": "^0.4.3",
    "magic-string": "^0.27.0",
    "unimport": "^2.1.0",
    "unplugin": "^1.0.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.1",
    "@types/node": "^18.11.19",
    "@types/resolve": "^1.20.2",
    "bumpp": "^8.2.1",
    "eslint": "^8.33.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "rollup": "^3.14.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "webpack": "^5.75.0"
  }
}
